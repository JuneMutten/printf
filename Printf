#include<stdio.h>
#include<stdarg.h>

int _printf(const char *format,...)
	{
		va_list args;
		va_start(args,format);
	int Count =0;
	while(*format!='\0')
	{
	if(*format =='%')
	format ++;
	if(*format =='C')
	{
			int c= va_arg(args,int);
			putchar(c);
			count ++;
				}
				elseif(*format =='S')
				{
			char*s=va_arg(args, char*);
			while(*S!='\0')
				}
			putchar(*s);
			s++
			count ++;
			}
				putchar('%');
				count++;
				}
				}
				else
				{
					putchar(*format);
					count ++;
					}
					format++;
					}
					va_end(args)
					return count;
		}
